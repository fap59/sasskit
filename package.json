{
  "name": "sass-kit",
  "displayName": "Sass Kit",
  "description": "Sass coding, simplified: auto-import and autocomplete in one tool.",
  "icon": "logo.png",
  "version": "1.3.0",
  "publisher": "fap59",
  "keywords": [
    "sass",
    "scss",
    "import",
    "use",
    "style",
    "add use to sass",
    "add use to scss",
    "sass-kit",
    "Sass Kit",
    "sass kit",
    "Sass-Kit",
    "Sass kit",
    "sass Kit",
    "sass_kit",
    "sasskit",
    "SassKit",
    "sassKit",
    "Sass IntelliSense",
    "Sass-IntelliSense",
    "sass intelliSense",
    "sass-intelliSense",
    "Scss IntelliSense",
    "Scss-IntelliSense",
    "Scss IntelliSense",
    "scss-intelliSense"
  ],
  "engines": {
    "vscode": "^1.80.0"
  },
  "main": "./out/extension",
  "activationEvents": [
    "onLanguage:sass",
    "onLanguage:scss"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.refreshSassCache",
        "title": "Sass Kit: Refresh Cache"
      }
    ],
    "languages": [
      {
        "id": "scss",
        "extensions": [".scss"]
      },
      {
        "id": "sass",
        "extensions": [".sass"]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/vscode": "^1.50.0",
    "typescript": "^4.5.0",
    "vsce": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fap59/sass-kit"
  },
  "enableProposedApi": true,
  "enable-proposed-api": ["fap59.sass-kit"]
}